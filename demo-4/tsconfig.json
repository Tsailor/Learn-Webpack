{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    // "module": "commonjs",
    "module": "es6",
    "target": "es5",
    "jsx": "preserve",
    "moduleResolution": "node",
    "experimentalDecorators":true,
    // "target": "es5",
    // "jsx": "react",
    "allowJs": false,
    "allowSyntheticDefaultImports": true
  },
  "include": ["./src/**/*"],
  "exclude": ["node_modules"],
}
